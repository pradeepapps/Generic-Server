{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "disable-next-line": false,
        "ordered-imports": false,
        "member-ordering": false,
        "object-literal-shorthand": false,
        "object-literal-sort-keys": false,
        "trailing-comma": false,
        "max-line-length": [true, 120],
        "quotemark": [
            true,
            "single"
        ]
    },
    "rulesDirectory": []
}